{
  "author": "Dead Pixels Collective",
  "bugs": {
    "url": "https://github.com/deathbeds/jyve/issues"
  },
  "peerDependencies": {
    "@deathbeds/jyve": "^0.2.0"
  },
  "dependencies": {
    "@deathbeds/jyve-coffee-unsafe": "^0.2.0",
    "@jupyterlab/application": "^0.15.4",
    "tslib": "^1.8.1"
  },
  "description": "An extension for a totally unsafe browser js kernel",
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "homepage": "https://github.com/deathbeds/jyve",
  "jupyterlab": {
    "extension": true
  },
  "keywords": [
    "coffee",
    "coffeescript",
    "jupyter",
    "jupyterlab-extension",
    "jupyterlab",
    "jyve"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "@deathbeds/jyve-coffee-unsafe-extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/jyve.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lab:link": "jupyter labextension link . --no-build",
    "watch": "tsc --watch",
    "publish:local": "npm publish --registry http://localhost:4873"
  },
  "types": "lib/index.d.ts",
  "version": "0.2.0",
  "jyve": {
    "kernelspec": {
      "display_name": "CoffeeScript (unsafe) â€” Jyve",
      "name": "jyve-coffee-unsafe",
      "language": "coffeescript",
      "arvg": ["echo", "jyve"],
      "resources": {}
    },
    "language_info": {
      "codemirror_mode": {
        "name": "coffeescript"
      },
      "file_extension": ".coffee",
      "mimetype": "text/coffeescript",
      "name": "coffeescript",
      "nbconvert_exporter": "coffeescript",
      "pygments_lexer": "coffeescript",
      "version": "2.2.1"
    },
    "help_links": [
      {
        "text": "CoffeeScript",
        "url": "http://coffeescript.org/"
      }
    ]
  }
}
