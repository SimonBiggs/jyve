{
  "author": "Dead Pixels Collective",
  "bugs": {
    "url": "https://github.com/deathbeds/jyve/issues"
  },
  "dependencies": {
    "@deathbeds/jyve": "^0.1.0",
    "@jupyterlab/application": "^0.15.4"
  },
  "description": "A totally unsafe browser js kernel",
  "files": [
    "{lib,style}/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "homepage": "https://github.com/deathbeds/jyve",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jyve"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "@deathbeds/jyve-js-unsafe",
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/jyve.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lab:link": "jupyter labextension link . --no-build",
    "watch": "tsc --watch",
    "publish:local": "npm publish --registry http://localhost:4873"
  },
  "types": "lib/index.d.ts",
  "version": "0.1.0",
  "jyve": {
    "kernelspec": {
      "display_name": "JavaScript (unsafe)",
      "name": "jyve-js-unsafe",
      "language": "JavaScript",
      "arvg": ["echo", "jyve"],
      "resources": {}
    },
    "language_info": {
      "codemirror_mode": {
        "name": "javascript"
      },
      "file_extension": ".js",
      "mimetype": "text/javascript",
      "name": "javascript",
      "nbconvert_exporter": "javascript",
      "pygments_lexer": "javascript",
      "version": "ES2015"
    },
    "help_links": [
      {
        "text": "MDN Web Docs",
        "url": "https://developer.mozilla.org"
      },
      {
        "text": "Can I use ... ?",
        "url": "https://caniuse.com"
      }
    ]
  }
}
